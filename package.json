{
  "name": "movie_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "pod-repo-update": "cd ios && pod install --repo-update && cd ..",
    "pod-clean": "cd ios && pod cache clean --all && cd ..",
    "fix-gradle": "cd android && gradle wrapper && cd ..",
    "gradlew-clean": "cd android && .\\gradlew clean && cd ..",
    "metro-clean": "rm -r android/build && rm -r android/app/src/release/res && rm -r android/app/build/intermediates && watchman watch-del-all &&  rm -rf $TMPDIR/react-*",
    "assemble-release": "cd android && .\\gradlew assembleRelease && cd ..",
    "assemble-debug": "cd android && .\\gradlew assembleDebug && cd ..",
    "apk": "cd android && .\\gradlew assembleRelease && cd ..",
    "bpk": "cd android && .\\gradlew assembleDebug && cd ..",
    "aab": "cd android && .\\gradlew bundleRelease && cd ..",
    "android-reconnect": "adb reverse tcp:8081 tcp:8081"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.12",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react-native": "^0.72.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.10.1",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.4.2",
    "themoviedb": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^7.32.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
